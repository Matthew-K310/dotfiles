    {
      "complex_modifications": {
        "parameters": {
          "basic.to_if_alone_timeout_milliseconds": 150,
          "basic.to_if_held_down_threshold_milliseconds": 225
        },
        "rules": [
          {
            "description": "Map fn+H, J, K, L as arrrow keys.",
            "manipulators": [
              {
                "description": "fn+H to Left Arrow",
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": ["fn"],
                    "optional": ["any"]
                  }
                },
                "to": [{ "key_code": "left_arrow" }],
                "type": "basic"
              },
              {
                "description": "fn+J to Down Arrow",
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["fn"],
                    "optional": ["any"]
                  }
                },
                "to": [{ "key_code": "down_arrow" }],
                "type": "basic"
              },
              {
                "description": "fn+K to Up Arrow",
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["fn"],
                    "optional": ["any"]
                  }
                },
                "to": [{ "key_code": "up_arrow" }],
                "type": "basic"
              },
              {
                "description": "fn+L to Right Arrow",
                "from": {
                  "key_code": "l",
                  "modifiers": {
                    "mandatory": ["fn"],
                    "optional": ["any"]
                  }
                },
                "to": [{ "key_code": "right_arrow" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Launch apps by left option+letters.",
            "manipulators": [
              {
                "from": {
                  "key_code": "return_or_enter",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [{ "shell_command": "open -na Kitty" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "w",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [{ "shell_command": "open -na Librewolf" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "e",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [{ "shell_command": "open -na Thunderbird" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "m",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  { "shell_command": "/bin/bash ~/.config/scripts/launch_rmpc" }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "m",
                  "modifiers": { "mandatory": ["option", "shift"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash ~/.config/scripts/launch_profanity"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "n",
                  "modifiers": { "mandatory": ["option", "shift"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash ~/.config/scripts/launch_newsboat"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "r",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash ~/.config/scripts/launch_yazi"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "p",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash -c '/opt/homebrew/bin/mpc toggle'"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "period",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash -c '/opt/homebrew/bin/mpc next'"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "comma",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash -c '/opt/homebrew/bin/mpc prev'"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "equal_sign",
                  "modifiers": { "mandatory": ["option"] }
                },
                "to": [
                  {
                    "shell_command": "/bin/bash -c 'osascript -e \"set volume output volume ((output volume of (get volume settings)) + 5)\"'"
